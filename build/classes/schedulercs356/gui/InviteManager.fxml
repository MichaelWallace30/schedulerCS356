<AnchorPane id="AnchorPane" prefHeight="349.0" prefWidth="448.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="schedulercs356.controllers.InviteManagerController">
    <stylesheets>
        <URL value="@/schedulercs356/css/invitemanager.css" />
