   <children>
      <Button fx:id="acceptButton" layoutX="14.0" layoutY="310.0" mnemonicParsing="false" onAction="#onButtonAccept" text="Accept" />
      <Button fx:id="rejectButton" layoutX="382.0" layoutY="310.0" mnemonicParsing="false" onAction="#onRejectButton" text="Reject" />
      <Button fx:id="postponeButton" layoutX="198.0" layoutY="310.0" mnemonicParsing="false" onAction="#onButtonPostpone" text="Postpone" />
      <TableView fx:id="invitedMeetingsTable" layoutX="24.0" layoutY="14.0" prefHeight="282.0" prefWidth="400.0">
